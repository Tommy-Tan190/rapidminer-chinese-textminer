<?xml version="1.0" encoding="windows-1252" standalone="no"?>
<operatorHelp lang="en_EN">
    <!--  This is an example how to specify the translation of operator keys to names. -->
    <operator>
      <key>CreateDocument</key>
      <name>Create Document</name>
    </operator>
    <operator>
        <key>WordSegmentation</key>
        <name>Word Segmentation</name>
    </operator>
    <operator>
        <key>Test</key>
        <name>Test</name>
    </operator>
    <operator>
        <key>Test1</key>
        <name>Test1</name>
    </operator>

    <!-- This is how group keys are translated: -->
    <group>
      <key>io</key>
      <name>IO</name>
    </group>


</operatorHelp>
