<?xml version="1.0" encoding="windows-1252" standalone="no"?>
<operatorHelp lang="en_EN">
    <!--  This is an example how to specify the translation of operator keys to names. -->
    <operator>
        <key>read_text</key>
        <name>Read Text</name>
        <synopsis>This operator can read text.</synopsis>
    </operator>
    <operator>
        <key>filter_stopwords</key>
        <name>Filtering</name>
    </operator>
    <operator>
        <key>tokenization</key>
        <name>Tokenization</name>
    </operator>
    <operator>
        <key>test</key>
        <name>Test</name>
    </operator>
    <operator>
        <key>test1</key>
        <name>Test1</name>
    </operator>

    <!-- This is how group keys are translated: -->
    <group>
        <key>io</key>
        <name>IO</name>
    </group>
    <group>
        <key>preprocessing</key>
        <name>Preprocessing</name>
    </group>

</operatorHelp>
